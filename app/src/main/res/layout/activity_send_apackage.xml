<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Send_a_package">


    <LinearLayout
        android:id="@+id/topPanel"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingBottom="20dp"
        android:layout_marginTop="20dp"
        app:layout_constraintTop_toTopOf="parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/imageView6"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_marginStart="20dp"
            android:onClick="backtohome"
            app:srcCompat="@drawable/back"
            tools:ignore="ContentDescription" />

        <TextView
            android:id="@+id/textView26"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginStart="110dp"
            android:layout_weight="1"
            android:text="Send a package"
            android:textColor="#A7a7a7"
            android:textSize="16sp"
            android:textStyle="bold"
            tools:ignore="HardcodedText,InefficientWeight,NestedWeights" />



        </LinearLayout>
    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="3dp"
        android:background="#CFCFCF"
        app:layout_constraintTop_toBottomOf="@+id/topPanel" />

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintTop_toBottomOf="@+id/topPanel"
        app:layout_constraintBottom_toBottomOf="parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginRight="20dp"
            android:orientation="vertical">


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginStart="24dp"
                android:layout_marginTop="43dp"
                android:orientation="vertical">


                <ImageView
                    android:id="@+id/imageView5"
                    android:layout_width="109dp"
                    android:layout_height="16dp"
                    android:layout_marginStart="24dp"
                    android:layout_marginEnd="350dp"
                    android:src="@drawable/soln"
                    android:contentDescription="TODO"
                    tools:ignore="ContentDescription,HardcodedText" />

                <EditText
                    android:id="@+id/address"
                    android:layout_width="342dp"
                    android:layout_height="48dp"
                    android:layout_gravity="center"
                    android:layout_marginTop="5dp"
                    android:background="@drawable/kv"
                    android:ems="10"
                    android:hint="Address"
                    android:inputType="textPersonName"
                    android:paddingStart="10dp"
                    android:textColorHint="#CFCFCF"
                    android:textSize="14sp"
                    tools:ignore="DuplicateSpeakableTextCheck,HardcodedText,RtlSymmetry,TextContrastCheck,TextSizeCheck"
                    android:autofillHints="" />

                <EditText
                    android:id="@+id/town"
                    android:layout_width="342dp"
                    android:layout_height="48dp"
                    android:layout_gravity="center"
                    android:layout_marginTop="5dp"
                    android:autofillHints=""
                    android:background="@drawable/kv"
                    android:ems="10"
                    android:hint="State,Country"
                    android:inputType="textPersonName"
                    android:paddingStart="10dp"
                    android:textColorHint="#CFCFCF"
                    android:textSize="14sp"
                    tools:ignore="HardcodedText,RtlSymmetry,TextSizeCheck,DuplicateSpeakableTextCheck,TextContrastCheck" />

                <EditText
                    android:id="@+id/phoneinfo"
                    android:layout_width="342dp"
                    android:layout_height="48dp"
                    android:layout_gravity="center"
                    android:layout_marginTop="5dp"
                    android:background="@drawable/kv"
                    android:ems="10"
                    android:hint="Phone number"
                    android:inputType="textPersonName"
                    android:paddingStart="10dp"
                    android:textColorHint="#CFCFCF"
                    android:textSize="14sp"
                    tools:ignore="DuplicateSpeakableTextCheck,HardcodedText,RtlSymmetry,TextContrastCheck,TextSizeCheck"
                    android:autofillHints="" />

                <EditText
                    android:id="@+id/jjj"
                    android:layout_width="342dp"
                    android:layout_height="48dp"
                    android:layout_gravity="center"
                    android:layout_marginTop="5dp"
                    android:autofillHints=""
                    android:background="@drawable/kv"
                    android:ems="10"
                    android:hint="Others"
                    android:inputType="textPersonName"
                    android:paddingStart="10dp"
                    android:textColorHint="#CFCFCF"
                    android:textSize="14sp"
                    tools:ignore="HardcodedText,RtlSymmetry,TextContrastCheck,TextSizeCheck,DuplicateSpeakableTextCheck" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginStart="24dp"
                android:layout_marginTop="39dp"
                android:orientation="vertical">

                <ImageView
                    android:id="@+id/uuu"
                    android:layout_width="119dp"
                    android:layout_height="16dp"
                    android:layout_marginStart="24dp"
                    android:src="@drawable/geo"
                    android:contentDescription="TODO"
                    tools:ignore="ContentDescription,HardcodedText" />

                <EditText
                    android:id="@+id/addresspoint1"
                    android:layout_width="342dp"
                    android:layout_height="48dp"
                    android:layout_gravity="center"
                    android:layout_marginTop="5dp"
                    android:autofillHints=""
                    android:background="@drawable/kv"
                    android:ems="10"
                    android:hint="Address"
                    android:inputType="textPersonName"
                    android:paddingStart="10dp"
                    android:textColorHint="#CFCFCF"
                    android:textSize="14sp"
                    tools:ignore="HardcodedText,RtlSymmetry,TextContrastCheck,TextSizeCheck" />

                <EditText
                    android:id="@+id/townpoint1"
                    android:layout_width="342dp"
                    android:layout_height="48dp"
                    android:layout_gravity="center"
                    android:layout_marginTop="5dp"
                    android:background="@drawable/kv"
                    android:ems="10"
                    android:hint="State,Country"
                    android:inputType="textPersonName"
                    android:paddingStart="10dp"
                    android:textColorHint="#CFCFCF"
                    android:textSize="14sp"
                    tools:ignore="HardcodedText,RtlSymmetry,TextContrastCheck,TextSizeCheck"
                    android:autofillHints="" />

                <EditText
                    android:id="@+id/phoneinfopoint1"
                    android:layout_width="342dp"
                    android:layout_height="48dp"
                    android:layout_gravity="center"
                    android:layout_marginTop="5dp"
                    android:autofillHints=""
                    android:background="@drawable/kv"
                    android:ems="10"
                    android:hint="Phone number"
                    android:inputType="textPersonName"
                    android:paddingStart="10dp"
                    android:textColorHint="#CFCFCF"
                    android:textSize="14sp"
                    tools:ignore="HardcodedText,RtlSymmetry,TextSizeCheck,TextContrastCheck" />

                <EditText
                    android:id="@+id/www"
                    android:layout_width="342dp"
                    android:layout_height="48dp"
                    android:layout_gravity="center"
                    android:layout_marginTop="5dp"
                    android:background="@drawable/kv"
                    android:ems="10"
                    android:hint="Others"
                    android:inputType="textPersonName"
                    android:paddingStart="10dp"
                    android:textColorHint="#CFCFCF"
                    android:textSize="14sp"
                    tools:ignore="HardcodedText,RtlSymmetry,TextContrastCheck,TextSizeCheck"
                    android:autofillHints="" />

                <ImageView
                    android:id="@+id/qqq"
                    android:layout_width="105dp"
                    android:layout_height="16dp"
                    android:layout_marginStart="24dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginEnd="350dp"
                    android:paddingStart="10dp"
                    android:src="@drawable/addgeo"
                    tools:ignore="ContentDescription,HardcodedText,ImageContrastCheck,RtlSymmetry"
                    android:contentDescription="TODO" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginStart="24dp"
                android:layout_marginTop="13dp"
                android:orientation="vertical">

                <ImageView
                    android:id="@+id/vvvv"
                    android:layout_width="101dp"
                    android:layout_height="16dp"
                    android:layout_marginStart="24dp"
                    android:layout_marginEnd="350dp"
                    android:paddingStart="10dp"
                    android:src="@drawable/packed"
                    android:contentDescription="TODO"
                    tools:ignore="ContentDescription,HardcodedText,RtlSymmetry" />

                <EditText
                    android:id="@+id/items"
                    android:layout_width="342dp"
                    android:layout_height="48dp"
                    android:layout_gravity="center"
                    android:layout_marginTop="5dp"
                    android:background="@drawable/kv"
                    android:ems="10"
                    android:hint="Address"
                    android:inputType="textPersonName"
                    android:paddingStart="10dp"
                    android:textColorHint="#CFCFCF"
                    android:textSize="14sp"
                    tools:ignore="HardcodedText,RtlSymmetry,TextSizeCheck"
                    android:autofillHints="" />

                <EditText
                    android:id="@+id/weightpack"
                    android:layout_width="342dp"
                    android:layout_height="48dp"
                    android:layout_gravity="center"
                    android:layout_marginTop="5dp"
                    android:autofillHints=""
                    android:background="@drawable/kv"
                    android:ems="10"
                    android:hint="State,Country"
                    android:inputType="textPersonName"
                    android:paddingStart="10dp"
                    android:textColorHint="#CFCFCF"
                    android:textSize="14sp"
                    tools:ignore="HardcodedText,RtlSymmetry,TextContrastCheck,TextSizeCheck" />

                <EditText
                    android:id="@+id/worthpack"
                    android:layout_width="342dp"
                    android:layout_height="48dp"
                    android:layout_gravity="center"
                    android:layout_marginTop="5dp"
                    android:background="@drawable/kv"
                    android:ems="10"
                    android:hint="Phone number"
                    android:inputType="textPersonName"
                    android:paddingStart="10dp"
                    android:textColorHint="#CFCFCF"
                    android:textSize="14sp"
                    tools:ignore="HardcodedText,RtlSymmetry,TextContrastCheck,TextSizeCheck"
                    android:autofillHints="" />

                <EditText
                    android:id="@+id/ffff"
                    android:layout_width="342dp"
                    android:layout_height="32dp"
                    android:layout_gravity="center"
                    android:layout_marginTop="5dp"
                    android:background="@drawable/kv"
                    android:ems="10"
                    android:hint="Others"
                    android:inputType="textPersonName"
                    android:paddingStart="10dp"
                    android:textColorHint="#CFCFCF"
                    android:textSize="14sp"
                    tools:ignore="HardcodedText,RtlSymmetry,TextContrastCheck,TextSizeCheck"
                    android:autofillHints="" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal"
                android:layout_marginStart="40dp"
                android:layout_marginTop="39dp">

                <TextView
                    android:id="@+id/textView37"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="Select delivery type"
                    android:textStyle="bold"
                    tools:ignore="HardcodedText" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:layout_marginTop="16dp"
                android:layout_marginStart="24dp">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal"
                    tools:ignore="UselessParent"
                    android:layout_marginBottom="20dp">


                    <ImageButton
                        android:id="@+id/imageButton11"
                        android:layout_width="159dp"
                        android:layout_height="75dp"
                        android:layout_weight="1"
                        android:background="@android:color/transparent"
                        android:contentDescription="TODO"
                        android:contextClickable="false"
                        android:onClick="next_event"
                        android:src="@drawable/inst"
                        tools:ignore="ContentDescription,HardcodedText" />

                    <ImageButton
                        android:id="@+id/imageButton12"
                        android:layout_width="159dp"
                        android:layout_height="75dp"
                        android:layout_weight="1"
                        android:background="@android:color/transparent"
                        android:src="@drawable/scheduled"
                        tools:ignore="ContentDescription,HardcodedText,ImageContrastCheck"
                        android:contentDescription="TODO" />

                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>